  "keywords": [
    "bank",
    "ai",
    "openai",
    "gpt-4",
    "loan",
    "deposit",
    "finance",
    "fintech",
    "korea",
    "recommendation"
  ],
  "author": "ConnectDot AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/connectdot/bank-ai-system.git"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "openai": "^4.55.3",
    "joi": "^17.13.3",
    "winston": "^3.14.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "@types/node": "^20.14.15"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "config": {
    "port": 3000,
    "environment": "development"
