{
  "name": "connectdot-bank",
  "version": "1.0.0",
  "description": "ConnectDot Bank AI Agent 시스템 (예금/대출 AI 추천 및 분석)",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "build": "npm install && npm run lint",
    "docker:build": "docker build -t connectdot-bank .",
    "docker:run": "docker run -p 3000:3000 connectdot-bank"
